			<value description="Released for Procurement">R</value>
			<value description="Not for New Development">N</value>
			<value description="Obsolete with Replacement">O</value>
			<value description="Obsolete with No Replacement">X</value>
		</dropdown>

		<reference prompt="Replacement" ref="replacement" priority="690"/>

		<group prompt="Name" ref="name" priority="1000">
