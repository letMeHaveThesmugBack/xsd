			<text prompt="Modifier" ref="modifier" length="16" priority="-1000"/>
		</group>

		<text prompt="Description" ref="description" length="4096" priority="999"/>

		<text prompt="Modifications" ref="modifications" length="4096" priority="998"/>

		<text prompt="Material" ref="material" length="16" priority="990"/>
		
